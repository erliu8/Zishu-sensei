{
  "id": "parallel-workflow-001",
  "name": "Parallel Task Execution Workflow",
  "description": "Workflow demonstrating parallel step execution",
  "version": "1.0.0",
  "status": "published",
  "steps": [
    {
      "id": "step-init",
      "name": "Initialize",
      "step_type": "transform",
      "order": 0,
      "config": {
        "input_data": "{{workflow_input}}",
        "output_variable": "initialized_data"
      },
      "retry_config": {
        "max_retries": 1,
        "retry_delay_ms": 500,
        "backoff_multiplier": 1.0
      },
      "timeout_seconds": 10,
      "enabled": true
    },
    {
      "id": "step-task-a",
      "name": "Task A - Data Processing",
      "step_type": "api_call",
      "order": 1,
      "config": {
        "adapter_id": "processing-adapter-a",
        "endpoint": "/process",
        "method": "POST",
        "payload": {
          "data": "{{initialized_data}}"
        },
        "output_variable": "result_a"
      },
      "dependencies": ["step-init"],
      "retry_config": {
        "max_retries": 2,
        "retry_delay_ms": 1000,
        "backoff_multiplier": 2.0
      },
      "timeout_seconds": 30,
      "enabled": true
    },
    {
      "id": "step-task-b",
      "name": "Task B - Image Generation",
      "step_type": "api_call",
      "order": 1,
      "config": {
        "adapter_id": "image-adapter-b",
        "endpoint": "/generate",
        "method": "POST",
        "payload": {
          "prompt": "{{initialized_data}}"
        },
        "output_variable": "result_b"
      },
      "dependencies": ["step-init"],
      "retry_config": {
        "max_retries": 2,
        "retry_delay_ms": 1000,
        "backoff_multiplier": 2.0
      },
      "timeout_seconds": 45,
      "enabled": true
    },
    {
      "id": "step-task-c",
      "name": "Task C - Translation",
      "step_type": "api_call",
      "order": 1,
      "config": {
        "adapter_id": "translation-adapter-c",
        "endpoint": "/translate",
        "method": "POST",
        "payload": {
          "text": "{{initialized_data}}",
          "target_language": "en"
        },
        "output_variable": "result_c"
      },
      "dependencies": ["step-init"],
      "retry_config": {
        "max_retries": 2,
        "retry_delay_ms": 1000,
        "backoff_multiplier": 2.0
      },
      "timeout_seconds": 20,
      "enabled": true
    },
    {
      "id": "step-merge",
      "name": "Merge Results",
      "step_type": "transform",
      "order": 2,
      "config": {
        "operation": "merge",
        "inputs": {
          "result_a": "{{result_a}}",
          "result_b": "{{result_b}}",
          "result_c": "{{result_c}}"
        },
        "output_variable": "final_result"
      },
      "dependencies": ["step-task-a", "step-task-b", "step-task-c"],
      "retry_config": {
        "max_retries": 1,
        "retry_delay_ms": 500,
        "backoff_multiplier": 1.0
      },
      "timeout_seconds": 10,
      "enabled": true
    },
    {
      "id": "step-output",
      "name": "Output Final Result",
      "step_type": "output",
      "order": 3,
      "config": {
        "message": "{{final_result}}",
        "format": "json"
      },
      "dependencies": ["step-merge"],
      "retry_config": {
        "max_retries": 0,
        "retry_delay_ms": 0,
        "backoff_multiplier": 1.0
      },
      "timeout_seconds": 5,
      "enabled": true
    }
  ],
  "config": {
    "timeout_seconds": 180,
    "max_retries": 2,
    "retry_delay_ms": 1000,
    "parallel_execution": true,
    "error_handling": "continue_on_error",
    "logging_enabled": true,
    "telemetry_enabled": true
  },
  "trigger": {
    "trigger_type": "manual",
    "config": {}
  },
  "tags": ["parallel", "performance", "multi-task"],
  "category": "advanced",
  "is_template": false,
  "template_id": null,
  "created_at": 1704067200,
  "updated_at": 1704153600
}

