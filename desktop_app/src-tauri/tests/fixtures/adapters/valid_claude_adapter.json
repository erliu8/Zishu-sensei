{
  "id": "claude-3-opus",
  "name": "Claude 3 Opus Adapter",
  "version": "1.0.0",
  "adapter_type": "soft",
  "description": "Anthropic Claude 3 Opus model adapter",
  "author": "Zishu Team",
  "license": "MIT",
  "tags": ["anthropic", "claude", "chat", "llm"],
  "created_at": "2024-03-01T00:00:00Z",
  "updated_at": "2024-03-15T00:00:00Z",
  "capabilities": [
    {
      "name": "chat_completion",
      "description": "Support for chat completion",
      "version": "1.0.0",
      "parameters": {
        "max_tokens": 200000,
        "temperature": 1.0
      }
    },
    {
      "name": "vision",
      "description": "Support for image understanding",
      "version": "1.0.0",
      "parameters": {
        "enabled": true
      }
    }
  ],
  "compatibility": {
    "min_version": "1.0.0",
    "max_version": "2.0.0",
    "platforms": ["windows", "macos", "linux"],
    "python_versions": [">=3.9"]
  },
  "resource_requirements": {
    "min_memory_mb": 512,
    "recommended_memory_mb": 1024,
    "disk_space_mb": 150,
    "cpu_cores": 2,
    "gpu_required": false
  },
  "config_schema": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string",
        "description": "Anthropic API key"
      },
      "model": {
        "type": "string",
        "description": "Model name",
        "default": "claude-3-opus-20240229"
      },
      "max_tokens": {
        "type": "integer",
        "minimum": 1,
        "maximum": 4096,
        "default": 1024
      },
      "temperature": {
        "type": "number",
        "minimum": 0.0,
        "maximum": 1.0,
        "default": 1.0
      }
    },
    "required": ["api_key"]
  },
  "default_config": {
    "model": "claude-3-opus-20240229",
    "max_tokens": 1024,
    "temperature": 1.0,
    "top_p": 1.0,
    "timeout": 60,
    "retry_attempts": 3
  },
  "soft_adapter_config": {
    "prompt_template": "You are Claude, a helpful AI assistant.",
    "rag_enabled": false,
    "context_window": 200000
  },
  "enabled": true,
  "priority": "high",
  "auto_load": false,
  "enable_monitoring": true,
  "log_level": "DEBUG"
}

